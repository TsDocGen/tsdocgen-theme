{
  "name": "@tsdocgen/themes",
  "version": "0.0.22",
  "description": "Theme base for tsdocgen",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "exec": "ts-node ./scripts/tsdocgen",
    "build:clean": "rm -rf dist",
    "build:babel": "babel --out-dir dist --extensions '.ts,.tsx' src",
    "build:dts": "tsc --emitDeclarationOnly --sourceMap --declarationMap -P tsconfig.json",
    "build": "yarn build:clean && yarn build:babel && yarn build:dts",
    "prepublishOnly": "yarn run build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "release": "yarn publish --access public "
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run build"
    }
  },
  "author": "Zerry Hogan",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/react": "^6.4.9",
    "@tsdocgen/core": "^0.0.37",
    "@types/glob": "^7.1.4",
    "@types/lodash": "^4.14.173",
    "@types/node": "^16.9.1",
    "@types/react-helmet": "^6.1.2",
    "@types/styled-components": "^5.1.14",
    "babel-loader": "^8.2.3",
    "husky": "^7.0.2",
    "styled-components": "^5.3.3"
  },
  "dependencies": {
    "@types/react": "^17.0.21",
    "@types/react-dom": "^17.0.9",
    "antd": "^4.16.13",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "peerDependencies": {
    "react": "16.x || 17.x",
    "react-dom": "16.x || 17.x",
    "styled-components": "4.x || 5.x",
    "@tsdocgen/core": "0.x"
  },
  "files": [
    "dist",
    "ant-design.d.ts",
    "ant-design.js"
  ]
}
